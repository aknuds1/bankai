{
  "name": "@arve.knudsen/bankai",
  "version": "5.1.4",
  "description": "DIY asset server",
  "main": "index.js",
  "bin": {
    "bankai": "bin.js"
  },
  "scripts": {
    "start": "./bin.js example.js --open -p 8080",
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "test": "npm run lint && npm run deps && NODE_ENV=test node test/index.js",
    "test:cov": "npm run lint && npm run deps && NODE_ENV=test istanbul cover test/index.js",
    "browser-test:cov": "npm run lint && npm run deps && NODE_ENV=test node test/test-live.js",
    "lint": "standard"
  },
  "repository": "aknuds1/bankai",
  "keywords": [
    "server",
    "dev",
    "development",
    "asset",
    "browserify",
    "sheetify",
    "html",
    "css",
    "js",
    "stream"
  ],
  "license": "MIT",
  "dependencies": {
    "bole": "^3.0.1",
    "browserify": "^14.3.0",
    "concat-stream": "^1.5.2",
    "create-html": "^3.0.0",
    "css-extract": "^1.2.0",
    "explain-error": "^1.0.3",
    "from2": "^2.3.0",
    "garnish": "^5.2.0",
    "map-limit": "0.0.1",
    "mkdirp": "^0.5.1",
    "opn": "^5.0.0",
    "pump": "^1.0.1",
    "resolve": "^1.1.7",
    "sheetify": "6.1.0",
    "subarg": "^1.0.0",
    "watchify": "^3.7.0",
    "watchify-request": "^2.1.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "bel": "^4.5.0",
    "bluebird": "^3.4.6",
    "choo": "^5.4.0",
    "dependency-check": "^2.6.0",
    "get-server-port": "^1.0.0",
    "is-html": "^1.0.0",
    "istanbul": "^0.4.4",
    "openport": "0.0.4",
    "phantom": "^4.0.3",
    "proxyquire": "^1.7.10",
    "request": "^2.75.0",
    "sinon": "^2.2.0",
    "standard": "^10.0.2",
    "tape": "^4.6.0"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**"
    ]
  }
}
